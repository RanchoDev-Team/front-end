{
  "homepage": "https://ranchodev-team.github.io/front-end",
  "name": "front-end",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "@owaiswiz/use-in-view": "^1.0.16",
    "feather-icons": "^4.28.0",
    "framer-motion": "^4.1.5",
    "react": "^17.0.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.2",
    "react-github-btn": "^1.2.0",
    "react-modal": "^3.13.1",
    "react-rnd": "^10.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.2.3",
    "tailwindcss": "^2.1.1",
    "twin.macro": "^2.3.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "dev": "react-scripts start",
    "predeploy": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "src/**/*.js"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.0"
  }
}
